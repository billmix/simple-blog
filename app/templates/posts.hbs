<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-12">
            <h1 class="text-center">Get Yer Posts Here</h1>
        </div>
    </div>
    {{outlet}}
    <div class="row">
        {{#each model as |post|}}
          {{#if post.post_published }}
          <div class="col-md-4">
            {{post-card featured_image=post.featured_image route=post title=post.title created_at=post.created_at content=post.content}}
          </div>
          {{/if}}
        {{/each}}

  </div>
</div>
